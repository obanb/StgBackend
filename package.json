{
  "name": "fptsbackend",
  "version": "0.1.0-alpha.1",
  "description": "functional ts backend examples",
  "author": "ondraba",
  "license": "ISC",
  "homepage": "https://",
  "repository": {
    "type": "git",
    "url": ""
  },
  "scripts": {
    "build": "tsc",
    "dev": "npm run build && concurrently \"tsc --watch\" \"cross-env NODE_ENV=DEVELOPMENT nodemon\"",
    "debug": "set NODE_ENV=DEVELOPMENT&& npm run build && node %NODE_DEBUG_OPTION% dist/index.js\"",
    "prod": "npm run build && concurrently \"tsc --watch\" \"nodemon\"",
    "start": "node dist/index.js",
    "stop": "node stop dist/index.js",
    "tslint": "tslint -c tslint.json -p tsconfig.json",
    "tslint:check": "tslint-config-prettier-check ./tslint.json",
    "test": "jest --verbose --colors --runInBand --no-cache",
    "prettier:show": "prettier --list-different \"./**/*\"",
    "prettier:write": "prettier \"./**/*\" --write",
    "precommit": "pretty-quick --staged && npm run tslint",
    "coverage": "jest --coverage --runInBand",
    "generate:merge": "node -e \"console.log(require('graphql-import').importSchema('./schema/root.graphql'))\" > graphql.schema.graphql",
    "generate:json": "npm run generate:merge && apollo-codegen introspect-schema graphql.schema.graphql --output graphql.schema.json",
    "generate:typescript": "rimraf lib && mkdir lib && gql-gen && tsc --lib es2017,esnext.asynciterable --declaration true ./lib/index.ts && rimraf ./lib/index.ts",
    "build:graphql": "npm run generate:json && npm run generate:typescript && tsc"
  },
  "jest": {
    "transform": {
      ".(ts|tsx)": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "(src/__tests__/.*|\\.(test|spec))\\.(ts|tsx)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "collectCoverageFrom": [
      "src/**/*.ts"
    ]
  },
  "dependencies": {
    "@types/express": "4.16.0",
    "@types/express-graphql": "0.6.2",
    "@types/graphql": "14.0.3",
    "@types/jsonwebtoken": "^8.3.9",
    "@types/moment": "2.13.0",
    "@types/mongoose": "5.3.5",
    "@types/node": "10.12.15",
    "@types/node-fetch": "2.1.2",
    "@types/sanctuary": "^0.14.2",
    "@types/socket.io": "^2.1.8",
    "@types/uuid": "^3.4.5",
    "StgGraphql": "git+https://github.com/obanb/StgGraphql",
    "apollo-cache-inmemory": "1.3.0",
    "apollo-client": "2.4.0",
    "apollo-codegen": "0.20.2",
    "apollo-link": "1.2.2",
    "apollo-link-http": "1.5.4",
    "apollo-link-retry": "2.2.4",
    "apollo-server-express": "2.3.1",
    "axios": "^0.18.0",
    "dataloader": "^2.0.0",
    "dotenv": "6.2.0",
    "express": "4.16.4",
    "express-graphql": "0.7.1",
    "fp-ts": "^2.1.0",
    "fp-ts-contrib": "^0.1.15",
    "graphql": "14.0.2",
    "graphql-code-generator": "0.18.2",
    "graphql-codegen-fragment-matcher": "0.18.2",
    "graphql-codegen-typescript-template": "0.18.0",
    "graphql-import": "0.7.1",
    "graphql-schema-tools": "0.0.6",
    "graphql-tools": "4.0.6",
    "http-status-codes": "^1.4.0",
    "hyper-ts": "^0.5.3",
    "io-ts": "^2.0.1",
    "io-ts-types": "^0.5.2",
    "jsonwebtoken": "^8.5.1",
    "lodash": "4.17.10",
    "moment": "2.23.0",
    "mongo-mock": "^4.0.0",
    "mongodb": "^3.6.3",
    "mongoose": "5.4.0",
    "monocle-ts": "^2.0.0",
    "newtype-ts": "^0.3.2",
    "node-fetch": "2.2.0",
    "ramda": "^0.26.1",
    "sanctuary": "^2.0.0",
    "save": "^2.4.0",
    "socket.io": "^2.3.0",
    "typescript-logging": "^0.6.3",
    "uuid": "^3.3.3"
  },
  "devDependencies": {
    "@types/jest": "23.3.1",
    "@types/lodash": "4.14.116",
    "@types/mongodb": "^3.6.0",
    "@types/ramda": "^0.25.43",
    "concurrently": "4.1.0",
    "cross-env": "5.2.0",
    "husky": "1.2.1",
    "jest": "23.5.0",
    "moxios": "^0.4.0",
    "nock": "^11.7.0",
    "nodemon": "1.18.9",
    "prettier": "1.15.3",
    "pretty-quick": "1.8.0",
    "rimraf": "2.6.2",
    "ts-jest": "23.10.5",
    "tslint": "5.12.0",
    "tslint-config-prettier": "1.17.0",
    "typescript": "3.5.2"
  }
}
